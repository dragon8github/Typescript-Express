{
    "type": "sqlite",
    "database": "db.sqlite3",
    "autoSchemaSync": true,
    "synchronize": true,
    "logging": true,
    "entities": [
        "src/entity/*.js"
    ],
    "migrations": [
        "src/migration/*.js"
    ],
    "subscribers": [
        "src/subscriber/*.js"
    ],
    "cli": {
        "entitiesDir": "src/entity",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}